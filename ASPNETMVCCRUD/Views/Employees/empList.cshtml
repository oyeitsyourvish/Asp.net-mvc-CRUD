
@model List<ASPNETMVCCRUD.Models.Domains.Employee>

<h1>This is Employee List Page.</h1>
<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Name</th>
			<th>Email</th>
			<th>Salary</th>
			<th>DateOfBirth</th>
			<th>Department</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var employee in Model)
		{
			<tr>
				<td>@employee.Id</td>
				<td>@employee.Name</td>
				<td>@employee.Email</td>
				<td>@employee.Salary</td>
				<td>@employee.DateOfBirth</td>
				<td>@employee.Department</td>
			</tr>
		}
	</tbody>
</table>